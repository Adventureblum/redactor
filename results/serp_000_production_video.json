    .production-map-toggle-buttons-button {\r\n        background-color: #1D1D1F;\r\n    }\r\n}\r\n.production-map-toggle-buttons-button .round-toggle {\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  width: 152px;\r\n  height: 100%;\r\n  background-color: #fff;\r\n  border-radius: 100px;\r\n  transition: left .4s;\r\n}\r\n.production-map-toggle-buttons-button .round-toggle.active {\r\n  left: calc(100% - 152px);\r\n}\r\n.production-map-toggle-buttons button {\r\n  background-color: transparent;\r\n  border: 0;\r\n  color: #fff;\r\n  padding: 8px 16px;\r\n  width: 152px;\r\n  font-family: \"proxima-nova\";\r\n  font-size: 14px;\r\n  font-style: normal;\r\n  font-weight: 600;\r\n  line-height: 24px; \r\n  letter-spacing: 0.4px;\r\n  border-radius: 100px;\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\n.production-map-toggle-buttons button.active {\r\n  color: black;\r\n}\r\n.production-map-toggle-buttons button:hover {\r\n  cursor: pointer;\r\n}\r\n.production-map {\r\n    position: absolute;\r\n    opacity: 0;\r\n    transition: opacity .4s;\r\n}\r\n.production-map.active {\r\n    opacity: 1;\r\n}\r\n</style>\r\n<div class=\"production-map-toggle-buttons\">\r\n    <div class=\"production-map-toggle-buttons-button\">\r\n        <div class=\"round-toggle\"></div>\r\n        <button id=\"national-button\" class=\"active\">\r\n          National Map\r\n        </button>\r\n        <button id=\"international-button\">\r\n          International Map\r\n        </button>\r\n    </div>\r\n</div>\r\n<script>\r\ndocument.addEventListener('DOMContentLoaded', () => {\r\n  const internationalButton = document.getElementById('international-button');\r\n  const nationalButton = document.getElementById('national-button');\r\n  const roundToggle = document.querySelector('.round-toggle');\r\n  const maps = document.querySelectorAll('.production-map');\r\n  \r\n  function toggleMaps() {\r\n      maps.forEach((elm) => {\r\n          elm.classList.toggle('active');\r\n      });\r\n  };\r\n\r\n  internationalButton.addEventListener('click', () => {\r\n    if (internationalButton.classList.contains(\"active\")) return;\r\n    \r\n    nationalButton.classList.remove('active');\r\n    internationalButton.classList.add('active');\r\n    roundToggle.classList.toggle('active');\r\n    toggleMaps();\r\n  });\r\n  nationalButton.addEventListener('click', () => {\r\n    if (nationalButton.classList.contains(\"active\")) return;\r\n    internationalButton.classList.remove('active');\r\n    nationalButton.classList.add('active');\r\n    roundToggle.classList.toggle('active');\r\n    toggleMaps();\r\n  });\r\n});\r\n</script>\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t<div class=\"elementor-element elementor-element-47a04b5b e-con-full e-flex e-con e-child\" data-id=\"47a04b5b\" data-element_type=\"container\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t\t<div class=\"elementor-element elementor-element-671c6e98 elementor-widget__width-inherit production-map active elementor-widget elementor-widget-html\" data-id=\"671c6e98\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<img width=\"1088\" height=\"672\" decoding=\"async\" src=\"data:image/svg+xml,%3Csvg%20xmlns='http://www.w3.org/2000/svg'%20viewBox='0%200%201088%20672'%3E%3C/svg%3E\" alt=\"Map US\" data-lazy-src=\"wp-content/uploads/2024/08/production-map-us.svg\" /><noscript><img width=\"1088\" height=\"672\" decoding=\"async\" src=\"wp-content/uploads/2024/08/production-map-us.svg\" alt=\"Map US\" /></noscript>\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"elementor-element elementor-element-7044796 elementor-widget__width-initial production-map elementor-widget elementor-widget-html\" data-id=\"7044796\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<img width=\"1115\" height=\"550\" decoding=\"