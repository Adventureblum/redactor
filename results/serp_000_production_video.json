lement elementor-element-1818d1d7 e-flex e-con-boxed e-con e-parent\" data-id=\"1818d1d7\" data-element_type=\"container\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;}\">\n\t\t\t\t\t<div  class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-e3ad24c elementor-widget elementor-widget-heading\" data-id=\"e3ad24c\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<h2 class=\"elementor-heading-title elementor-size-default\">Let’s talk video.</h2>\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"elementor-element elementor-element-7c5a83af elementor-widget elementor-widget-html\" data-id=\"7c5a83af\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<div class=\"form-header\">\r\n  <p>Please fill out the form below, and an Account Executive will follow up within 2 hours (Business hours are Monday–Friday, 8 AM–6 PM). If you're trying to reach someone else, please visit our <a href=\"/contact/\">contact page</a>.</p>\r\n</div>\r\n<br/>\r\n\r\n\r\n<div id=\"contactform\">\r\n  <script>\r\n    hbspt.forms.create({\r\n      region: \"na1\",\r\n      portalId: \"1697659\",\r\n      target: \".contactformcontainer\",\r\n      formId: \"a8fd798e-3b44-4bcc-b148-5cc176f577f0\",\r\n      onFormSubmit: function($form) {\r\n\r\n        function params() {\r\n          const url = new URL(location.href);\r\n          const result = [];\r\n\r\n          // Default UTM values\r\n          const defaultParams = {\r\n            utm_campaign: 'website-get-started',\r\n            utm_source: 'website',\r\n            utm_medium: currentPathPopUpLL,\r\n          };\r\n\r\n          // Add UTM parameters\r\n          Object.keys(defaultParams).forEach((param) => {\r\n            result.push(`${param}=${encodeURIComponent(url.searchParams.get(param) || defaultParams[param])}`);\r\n          });\r\n\r\n          // Add form field values\r\n          const formFields = ['firstname', 'lastname', 'email', 'phone', 'company'];\r\n          formFields.forEach(name => {\r\n            const input = document.querySelector(`[name=\"${name}\"]`);\r\n            if (input && input.value) {\r\n              result.push(`${name}=${encodeURIComponent(input.value)}`);\r\n            }\r\n          });\r\n\r\n          return result.join('&');\r\n        }\r\n\r\n        setTimeout( function() {\r\n          var interestedIn = $form.find('select[name=\"i_m_interested_in_\"]').val();\r\n          if(interestedIn === \"Filming a project for my company\"){\r\n            var numberEmployee = $form.find('select[name=\"number_of_employees_\"]').val();\r\n            switch (numberEmployee){\r\n              case \"50-199\":\r\n                window.location.href = `/brand-mm-get-started?${params()}`;\r\n                break;\r\n              case \"200-2499\":\r\n                window.location.href = `/brand-enterprise-get-started?${params()}`;\r\n                break;\r\n              case \"+2500\":\r\n                window.location.href = `/brand-enterprise-get-started?${params()}`;\r\n                break;\r\n              default:\r\n                window.location.href = `/brand-smb-get-started?${params()}`;\r\n            }\r\n          } else if(interestedIn === \"A potential partnership\") {\r\n            window.location.href = `/get-started-agency?${params()}`;\r\n          }\r\n        }, 250); // Redirects to url with query string data from form fields after 250 milliseconds.\r\n      }\r\n    });\r\n  </script>\r\n  <div class=\"contactformcontainer\"></div>\r\n</div>\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t</article>\n\t\t\t\t<div data-elementor-type=\"popup\" data-elementor-id=\"23673\" class=\"elementor elementor-23673 elementor-location-popup\" data-elementor-settings=\"{&quot;a11y_navigation&quot;:&quot;yes&quot;,&quot;t